    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.24.1",
    "del": "^3.0.0",
    "electron": "^10.1.3",
    "electron-builder": "19.41.0",
    "gulp": "^3.9.1",
    "gulp-babel": "^7.0.0",
    "gulp-babel-minify": "^0.2.0",
    "gulp-sourcemaps": "^2.6.5",
    "gulp-util": "^3.0.8",
    "gulp-watch": "^4.3.11",
    "vinyl-buffer": "^1.0.0",
